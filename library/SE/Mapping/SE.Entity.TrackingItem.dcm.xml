<doctrine-mapping  xmlns:xsi='http://www.w3.org/2001/XMLSchema-instance'
   xmlns='http://doctrine-project.org/schemas/orm/doctrine-mapping'
   xsi:schemaLocation='http://doctrine-project.org/schemas/orm/doctrine-mapping ../../../docs/doctrine-mapping.xsd'>

    <entity name="SE\Entity\TrackingItem" table="tracking_item">

        <id name="id" type="integer" column="trackeditem_id">
            <generator strategy="AUTO" />
        </id>
        <field name="requestDate" column="trackingitem_requestdate" type="date" nullable="false" />
        <field name="trackingDate" column="trackingitem_trackingdate" type="date" nullable="true" />
        <field name="updated" column="trackingitem_updateddate" type="date" nullable="false" />
        <field name="term" column="trackingitem_term" type="string" length="400" nullable="false" />
     </entity>

</doctrine-mapping>
